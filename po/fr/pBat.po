#: ../pbat/lang/pBat_ShowHelp.c:48
msgid ""
"Create an alias on a command.\n"
"Usage: ALIAS [/f] alias=command\n"
msgstr ""
"Crée un alias sur une commande.\n"
"Usage: ALIAS [/f] alias=command\n"

#: ../pbat/lang/pBat_ShowHelp.c:52
msgid ""
"Change current working directory.\n"
"Usage: CD  [[/d] path]\n"
"       CHDIR [[/d] path]\n"
msgstr ""
"Change le chemin courant.\n"
"Usage: CD [[/d] chemin]\n"
"       CHDIR [[/d] chemin]\n"

#: ../pbat/lang/pBat_ShowHelp.c:57
msgid ""
"Clear console screen\n"
"Usage: CLS\n"
msgstr ""
"Efface le contenu de la console\n"
"Usage: CLS\n"

#: ../pbat/lang/pBat_ShowHelp.c:61
msgid ""
"Change console color to the given code.\n"
"Usage: COLOR [code]\n"
msgstr ""
"Change la couleur de toute la console.\n"
"Usage: COLOR [code]\n"

#: ../pbat/lang/pBat_ShowHelp.c:65
msgid ""
"Copy file(s) to another location.\n"
"Usage: COPY [/R] [/-Y | /Y] [/A[:]attributes] source [+] ... destination\n"
msgstr ""
"Copie des fichiers vers un autre répertoire.\n"
"Usage: COPY [/R] [/-Y | /Y] [/A[:]attributs] source [+] ... destination\n"

#: ../pbat/lang/pBat_ShowHelp.c:69
msgid ""
"Call a script or a label from the current context.\n"
"Usage: CALL [/e] [file] [:label] [parameters ...]\n"
msgstr ""
"Appelle un script ou un étiquette depuis le contexte courant.\n"
"Usage: CALL [/e] [fichier] [:étiquette] [paramètres ...]\n"

#: ../pbat/lang/pBat_ShowHelp.c:74
msgid ""
"Delete file(s).\n"
"Usage: DEL [/P] [/F] [/S] [/Q] [/A[:]attributes] name\n"
"       ERASE [/P] [/F] [/S] [/Q] [/A[:]attributes] name\n"
msgstr ""
"Supprime des fichiers.\n"
"Usage: DEL [/P] [/F] [/S] [/Q] [/A[:]attributs] nom\n"
"       ERASE [/P] [/F] [/S] [/Q] [/A[:]attributes] nom\n"

#: ../pbat/lang/pBat_ShowHelp.c:79
msgid ""
"List files in a directory and in subdirectories.\n"
"Usage: DIR [/A[:]attributes] [/S] [/B] [path]\n"
msgstr ""
"Liste les fichiers d'un répertoire et de ses sous-répertoires.\n"
"Usage: DIR [/A[:]attributs] [/S] [/B] [chemin]\n"

#: ../pbat/lang/pBat_ShowHelp.c:83
msgid ""
"Print a message, or enable/disable command echoing.\n"
"Usage: ECHO [OFF|ON]\n"
"       ECHO text\n"
"       ECHO.text\n"
msgstr ""
"Affiche un message, ou active/désactive l'affichage des commandes.\n"
"Usage: ECHO [OFF|ON]\n"
"       ECHO texte\n"
"       ECHO.texte\n"

#: ../pbat/lang/pBat_ShowHelp.c:89
#, c-format
msgid ""
"Perform commands against items (files, strings, command output).\n"
"Usage: FOR %%A IN (string) DO (\n"
"          :: some commands\n"
"       )\n"
"\n"
"       FOR /F [options] %%A IN (object) DO (\n"
"          :: some commands\n"
"       )\n"
"\n"
"       FOR /L %%A IN (start,increment,end) DO (\n"
"          :: some commands\n"
"       )\n"
msgstr ""
"Effectue une boucle de commandes sur des éléments.\n"
"Usage: FOR %%A IN (chaîne) DO (\n"
"          :: des commandes\n"
"       )\n"
"\n"
"       FOR /F [options] %%A IN (objets) DO (\n"
"          :: des commandes\n"
"       )\n"
"\n"
"       FOR /L %%A IN (début,incrément,fin) DO (\n"
"          :: des commandes\n"
"       )\n"

#: ../pbat/lang/pBat_ShowHelp.c:101
msgid ""
"Go to a label in a command script.\n"
"Usage: GOTO [/Q] [/F[:]file] [:]label\n"
msgstr ""
"Saute vers une étiquette dans un script de commande.\n"
"Usage: GOTO [/Q] [/F[:]file] étiquette\n"

#: ../pbat/lang/pBat_ShowHelp.c:105
msgid ""
"Perform commands on some conditions.\n"
"Usage: IF [/i] [NOT] string1==string2 (\n"
"          :: code to be ran\n"
"       )\n"
"\n"
"       IF [/i] string1 cmp string2\n"
"          :: code to be ran\n"
"       )\n"
"\n"
"       IF [NOT] [DEFINED | EXIST | ERRORLEVEL] object (\n"
"          :: code to be ran\n"
"       )\n"
"       IF [ expression ] (\n"
"          :: code to be ran\n"
"       )\n"
msgstr ""
"Effectue des opérations conditionnelles de commandes.\n"
"Usage: IF [/i] [NOT] chaîne1==chaîne2 (\n"
"          :: code à exécuter\n"
"       )\n"
"\n"
"       IF [/i] chaîne1 cmp chaîne2\n"
"          :: code à exécuter\n"
"       )\n"
"\n"
"       IF [NOT] [DEFINED | EXIST | ERRORLEVEL] objet (\n"
"          :: code à exécuter\n"
"       )\n"
"       IF [ expression ] (\n"
"          :: code à exécuter\n"
"       )\n"

#: ../pbat/lang/pBat_ShowHelp.c:120
msgid ""
"Create the given directory.\n"
"Usage: MD directory\n"
"       MKDIR directory\n"
msgstr ""
"Crée un répertoire.\n"
"Usage: MD répertoire\n"
"       MKDIR répertoire\n"

#: ../pbat/lang/pBat_ShowHelp.c:126
msgid ""
"Wait for a keystroke from the user.\n"
"Usage: PAUSE\n"
msgstr ""
"Attend que l'utilisateur appuie sur une touche\n"
"Usage: PAUSE\n"

#: ../pbat/lang/pBat_ShowHelp.c:131
msgid ""
"Remove a directory.\n"
"Usage: RMDIR [/S] [/Q] directory\n"
"       RD [/S] [/Q] directory\n"
msgstr ""
"Supprime un répertoire\n"
"Usage: RMDIR [/S] [/Q] répertoire\n"
"       RD [/S] [/Q] répertoire\n"

#: ../pbat/lang/pBat_ShowHelp.c:136
msgid ""
"Renames a file.\n"
"Usage: REN file name.ext\n"
"       RENAME file name.ext\n"
msgstr ""
"Renomme un fichier.\n"
"Usage: REN fichier nom.ext\n"
"       RENAME fichier nom.ext\n"

#: ../pbat/lang/pBat_ShowHelp.c:141
msgid ""
"Introduce a comment.\n"
"Usage: REM comment\n"
msgstr ""
"Introduit un commentaire.\n"
"Usage: REM commentaire\n"

#: ../pbat/lang/pBat_ShowHelp.c:146
msgid ""
"Set or list environment variables.\n"
"Usage: SET variable=content\n"
"       SET /a[[:][i|f]] variable=expression\n"
"       SET /p variable=question\n"
"       SET var\n"
msgstr ""
"Affecte une variable d'environement.\n"
"Usage: SET variable=contenu\n"
"       SET /a[[:][i|f]] variable=expression\n"
"       SET /p variable=question\n"
"\t\tSET var\n"

#: ../pbat/lang/pBat_ShowHelp.c:153
#, fuzzy
msgid ""
"Set local environment and options of the pBat command prompt.\n"
"Usage: SETLOCAL [ENABLEDELAYEDEXPANSION | DISABLEDELAYEDEXPANSION] "
"[CMDLYCORRECT | CMDLYINCORRECT]\n"
msgstr ""
"Crée un environnement local et active ou désative les options de Dos9.\n"
"Usage: SETLOCAL [ENABLEDELAYEDEXPANSION | DISABLEDELAYEDEXPANSION] "
"[ENABLEFLOATS | DISABLEFLOATS] [CMDLYCORRECT | CMDLYINCORRECT]\n"

#: ../pbat/lang/pBat_ShowHelp.c:157
msgid ""
"Reverts changes to environments and options made after a call to SETLOCAL.\n"
"Usage: ENDLOCAL\n"
msgstr ""
"Anulle les changements de l'environnement et des options depuis le dernier "
"SETLOCAL.\n"
"Usage: ENDLOCAL\n"

#: ../pbat/lang/pBat_ShowHelp.c:161
msgid ""
"Set console title.\n"
"Usage: TITLE title\n"
msgstr ""
"Modifie le titre de la console.\n"
"Usage: TITLE titre\n"

#: ../pbat/lang/pBat_ShowHelp.c:165
msgid ""
"Print a file.\n"
"Usage: TYPE [file ...]\n"
msgstr ""
"Affiche le contenu d'un fichier.\n"
"Usage: TYPE [fichier ...]\n"

#: ../pbat/lang/pBat_ShowHelp.c:169
msgid ""
"Exit from the command prompt.\n"
"Usage: EXIT [/b] [code]\n"
msgstr ""
"Quitte l'invité de commande.\n"
"Usage: EXIT [/b] [code]\n"

#: ../pbat/lang/pBat_ShowHelp.c:173
msgid ""
"Change positions of command-line parameters.\n"
"Usage: SHIFT [/start_number | /s[:]start_number] [/d[:]displacement]\n"
msgstr ""
"Change les positions des paramètres de ligne de commande.\n"
"Usage: SHIFT [/nombre_début | /s[:]nombre_début] [/d[:]déplacement\n"

#: ../pbat/lang/pBat_ShowHelp.c:177
msgid ""
"Search occurrence of string in a set of files.\n"
"Usage: FIND [/N] [/C] [/V] [/I] [/E] string [file ...]\n"
msgstr ""
"Recherche les occurences d'une chaîne dans un groupe de fichiers.\n"
"Usage: FIND [/N] [/C] [/V] [/I] [/E] chaîne [fichier ...]\n"

#: ../pbat/lang/pBat_ShowHelp.c:181
msgid ""
"Display content of files.\n"
"Usage: MORE [/E] [/P] [/S] [/C] [/T[:]n] [+n] [file ...]\n"
msgstr ""
"Affiche le contenu d'un ou plusieur fichiers.\n"
"Usage: MORE [/E] [/P] [/S] [/C] [/T[:]n] [+n] [fichier ...]\n"

#: ../pbat/lang/pBat_ShowHelp.c:185
msgid ""
"[Obsolete] Sets or Clears Extended CTRL+C checking on DOS system. (Do "
"nothing)\n"
"Usage: BREAK [ON | OFF]\n"
msgstr ""
"[Obsolète] Active ou désactive le contrôle de CTRL+C sur les systèmesDOS. "
"(Ne fait rien)\n"
"Usage: Break [ON | OFF]\n"

#: ../pbat/lang/pBat_ShowHelp.c:189
msgid ""
"Set Console codepage or encoding.\n"
"Usage: CHCP [ codepage | encoding]\n"
msgstr ""
"Modifie l'encodage ou la page de code de la console de la console\n"
"Usage: CHCP [ page_code | encodage]\n"

#: ../pbat/lang/pBat_ShowHelp.c:193
msgid ""
"For each path, change current directory and pushd to stack\n"
"the previous current directory.\n"
"Usage: PUSHD [path1] [path2] [pathN] ...\n"
msgstr ""
"Pour chaque chemin, change le répertoire courant et pousse\n"
"sur la pile le répertoire courant précedent.\n"
"Usage: PUSHD [chemin1] [chemin2] [cheminN] ...\n"

#: ../pbat/lang/pBat_ShowHelp.c:198
msgid ""
"Pull a directory on the stack to change current directory.\n"
"Usage: POPD\n"
msgstr ""
"Tire un répertoire sur la pile pour changer le répertoire courant.Usage: "
"POPD\n"

#: ../pbat/lang/pBat_ShowHelp.c:202
msgid ""
"Display counts of a file or of stdin.\n"
"Usage: WC [/L] [/C] [/M] [/W] [files ...]\n"
msgstr ""
"Affiche les comptes d'un fichier ou de l'entrée standard.\n"
"Usage: WC [/L] [/C] [/M] [/W] [fichiers ...]\n"

#: ../pbat/lang/pBat_ShowHelp.c:206
msgid ""
"Run a command with arguments from standard input file.\n"
"Usage: XARGS [command ...]\n"
msgstr ""
"Lance une commande en prenant les arguments sur l'entrée standard.\n"
"Usage: XARGS [commande ...]\n"

