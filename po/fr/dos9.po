#: ../dos9/errors/Dos9_Errors.c:42
msgid "No error: OK.\n"
msgstr "Aucune erreur: OK.\n"

#: ../dos9/errors/Dos9_Errors.c:45
#, c-format
msgid "Error : Unable to access file \"%s\".\n"
msgstr "Erreur : Impossible d'accéder au fichier \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:48
#, c-format
msgid "Error : Unable to find folder \"%s\".\n"
msgstr "Erreur : Impossible de trouver le dossier \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:51
#, c-format
msgid ""
"Error : \"%s\" is not recognized as an internal or external command, an "
"operable program or a batch file.\n"
msgstr ""
"Erreur : \"%s\" n'est pas reconnu en tant que commande interne ou externe, "
"programme exécutable ou fichier batch.\n"

#: ../dos9/errors/Dos9_Errors.c:55
#, c-format
msgid "Error : \"%s\" was unexpected.\n"
msgstr "Erreur : \"%s\" était inatendu.\n"

#: ../dos9/errors/Dos9_Errors.c:58
#, c-format
msgid "Error : Invalid command line for %s.\n"
msgstr "Erreur : Ligne de commande invalide \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:61
#, c-format
msgid "Error : Unable to find label \"%s\".\n"
msgstr "Erreur : Impossible de trouver l'étiquette \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:64
msgid "Warning : Using a Dos9 extension but the CMDLYCORRECT option is set.\n"
msgstr ""
"Attention : il faut désactiver l'option CMDLYCORRECT pour utiliser les "
"extensions de Dos9.\n"

#: ../dos9/errors/Dos9_Errors.c:67
#, c-format
msgid "Error : \"%s\" expected more arguments.\n"
msgstr "Erreur : \"%s\" attendait plus d'arguments.\n"

#: ../dos9/errors/Dos9_Errors.c:70
#, c-format
msgid "Error : Incompatible arguments %s.\n"
msgstr "Erreur : Arguments incompatibles %s.\n"

#: ../dos9/errors/Dos9_Errors.c:73
#, c-format
msgid "Error : Unable to rename \"%s\".\n"
msgstr "Erreur : Impossible de renommer \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:76
msgid "Error : Overflowed maximum value.\n"
msgstr "Erreur : Dépassement de valeur maximale.\n"

#: ../dos9/errors/Dos9_Errors.c:79
msgid "Error : Divide by 0.\n"
msgstr "Erreur : Division par 0\n"

#: ../dos9/errors/Dos9_Errors.c:82
#, c-format
msgid "Error : Unable to create folder \"%s\".\n"
msgstr "Erreur : Impossible de créer le dossier \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:85
#, c-format
msgid "Error : Unable to delete folder \"%s\".\n"
msgstr "Erreur : Impossible de suprimmer le dossier \"%s\"\n"

#: ../dos9/errors/Dos9_Errors.c:88
#, c-format
msgid "Error : Stream module : \"%s\".\n"
msgstr "Erreur : Module de flux : \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:91
#, c-format
msgid ""
"Error : \"%c\" is an invalid character for special variables. Special "
"variables require their name to be strict ascii characters, exluding "
"controls characters and space (0x00 - 0x30).\n"
msgstr ""
"Erreur : \"%c\" est un caractère invalide pour les variable spéciales. Le "
"nom variables spécial doit être un caractère ASCII strict, à l'exception des "
"caractères de contrôle et de l'espace (0x00-0x30).\n"

#: ../dos9/errors/Dos9_Errors.c:97
#, c-format
msgid ""
"Error : \"%s\" is not a valid block (should at least be enclosed within "
"parenthesis).\n"
msgstr ""
"Errur : \"%s\" n'est pas un bloc valable (devrait être entouré de "
"parenthèses).\n"

#: ../dos9/errors/Dos9_Errors.c:101
#, c-format
msgid "Error : \"%s\" is not a valid token specifier.\n"
msgstr "Erreur :   \"%s\" n'est pas une description de token valide.\n"

#: ../dos9/errors/Dos9_Errors.c:104
#, c-format
msgid ""
"Error : Attempted to specify more than TOKEN_NB_MAX (%d) different tokens.\n"
msgstr ""
"Erreur : La commande a tenté de spécifier plus de TOKEN_NB_MAX(%d) tokens "
"différent.\n"

#: ../dos9/errors/Dos9_Errors.c:108
#, c-format
msgid "Error : \"%s\" violates the ``Usebackq'' syntax.\n"
msgstr "Erreur : \"%s\" viole la syntaxe ``Usebackq''.\n"

#: ../dos9/errors/Dos9_Errors.c:111
#, c-format
msgid "Error : Unable to allocate memory (in %s).\n"
msgstr "Erreur : Impossible d'allouer de la mémoire (dans %s).\n"

#: ../dos9/errors/Dos9_Errors.c:114
#, c-format
msgid "Error : Can't create pipe (in %s).\n"
msgstr "Erreur : Impossible de créer un pipe (dans %s).\n"

#: ../dos9/errors/Dos9_Errors.c:117
#, c-format
msgid ""
"Error : unable to run a subprocess of Dos9 to process the given input (\"%s"
"\").\n"
msgstr ""
"Error : Impossible de lancer une seconde instance de Dos9 pour exécuter "
"l'entée spécifiée (\"%s\").\n"

#: ../dos9/errors/Dos9_Errors.c:121
#, c-format
msgid ""
"Error : Invalid token specifier. \"%s\" breaks input specifying rules.\n"
msgstr "Erreur : \"%s\" brise les règles de spécification de tokens.\n"

#: ../dos9/errors/Dos9_Errors.c:125
#, c-format
msgid "Error : FOR loop is trying to reassign already used %%%%%c variable.\n"
msgstr ""
"Erreur : La boucle FOR a essayé de réassigner la variable %%%%%c qui est "
"déjà utilisée.\n"

#: ../dos9/errors/Dos9_Errors.c:129
#, c-format
msgid "Error : \"%s\" is not a valid mathematical expression.\n"
msgstr "Erreur : \"%s\" n'est pas une expression mathématique valide.\n"

#: ../dos9/errors/Dos9_Errors.c:132
#, c-format
msgid "Error : \"%s\" is not a valid top-level block.\n"
msgstr "Erreur :   \"%s\" n'est pas un block de niveau supérieur valide.\n"

#: ../dos9/errors/Dos9_Errors.c:135
#, c-format
msgid "Error : Unable to duplicate file descriptor (%d).\n"
msgstr "Erreur : Impossible de duppliquer le descripteur de fichier  (%d).\n"

#: ../dos9/errors/Dos9_Errors.c:138
#, c-format
msgid "Error : Unable to create pipe at function (%s).\n"
msgstr "Erreur : Impossible de créer un pipe (dans %s).\n"

#: ../dos9/errors/Dos9_Errors.c:141
#, c-format
msgid "Error : Unable to set environment string (%s).\n"
msgstr ""
"Erreur : Impossible de modifier la variable d'environement (dans %s).\n"

#: ../dos9/errors/Dos9_Errors.c:144
#, c-format
msgid "Error : \"%s\" is an invalid stream redirection.\n"
msgstr "Erreur :   \"%s\" n'est pas une redirection valide.\n"

#: ../dos9/errors/Dos9_Errors.c:147
#, c-format
msgid "Error : Redirection for \"%s\" was already specified.\n"
msgstr "Erreur : Une redirection pour \"%s\" était déjà spécifiée.\n"

#: ../dos9/errors/Dos9_Errors.c:150
#, c-format
msgid "Error : Encountered malformed blocks (\"%s\").\n"
msgstr "Erreur : Mauvais bloc (\"%s\").\n"

#: ../dos9/errors/Dos9_Errors.c:153
msgid "Error : Blocks left unclosed at end-of-file.\n"
msgstr "Erreur : Bloc non fermé à la fin du fichier.\n"

#: ../dos9/errors/Dos9_Errors.c:156
#, c-format
msgid "Error : Unable to add command \"%s\".\n"
msgstr "Erreur : Impossible de renommer \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:159
#, c-format
msgid "Error : Unable to remap commands (in function %s).\n"
msgstr "Erreur : Impossible de remaper les commandes (dans %s).\n"

#: ../dos9/errors/Dos9_Errors.c:162
#, c-format
msgid "Error : Trying to redefine already used \"%s\" command.\n"
msgstr ""
"Erreur : Tentative de réassignement de la commande déjà utilisée \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:165
#, c-format
msgid "Error : Unable to redefine \"%s\" command.\n"
msgstr "Erreur : Impossible de redéfinir la commande \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:168
#, c-format
msgid ""
"Error : Unable to set option \"%s\", it is built statically inside your "
"version of Dos9.\n"
msgstr ""
"Erreur : Impossible d'activer l'option \"%s\" qui est compilée à l'intérieur "
"de Dos9.\n"

#: ../dos9/errors/Dos9_Errors.c:172
#, c-format
msgid "Error : The \"%s\" comparison forbids use of strings.\n"
msgstr "Erreur : Le comparant \"%s\" interdit l'usage de chaines.\n"

#: ../dos9/errors/Dos9_Errors.c:175
#, c-format
msgid "Error : Unable to fork the current process (in function %s).\n"
msgstr "Erreur : Impossible de forker le processus courant (dans %s).\n"

#: ../dos9/errors/Dos9_Errors.c:178
#, c-format
msgid "Error : Pathspec \"%s\" does not match any file.\n"
msgstr "Erreur : L'expression \"%s\" ne correspond à aucun fichier.\n"

#: ../dos9/errors/Dos9_Errors.c:181
#, c-format
msgid "Error : %s does not recieved any valid file to process.\n"
msgstr "Erreur : %s n'a reçu aucun fichier valide à traiter.\n"

#: ../dos9/errors/Dos9_Errors.c:184
#, c-format
msgid "Error : \"%s\" is not a valid number.\n"
msgstr "Erreur :   \"%s\" n'est pas un nombre valide.\n"

#: ../dos9/errors/Dos9_Errors.c:187
#, c-format
msgid "Error : Too many arguments for %s command\n"
msgstr "Erreur : Trop d'arguments pour la commande %s.\n"

#: ../dos9/errors/Dos9_Errors.c:190
#, c-format
msgid "Error : \"%s\" is not a valid expression for IF command.\n"
msgstr "Erreur : \"%s\" n'est pas une expression valide pour la commandeIF.\n"

#: ../dos9/errors/Dos9_Errors.c:193
#, c-format
msgid "Error : Unable to copy file \"%s\".\n"
msgstr "Erreur : Impossible de copier le fichier\"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:196
#, c-format
msgid "Error : Unable to move file \"%s\".\n"
msgstr "Erreur : Impossible de déplacer le fichier \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:199
#, c-format
msgid "Error : Unable to create directory \"%s\".\n"
msgstr "Erreur : Impossible de créer le dossier \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:202
#, c-format
msgid "Error : Unable to delete file \"%s\".\n"
msgstr "Erreur : Impossible de suprimmer le fichier \"%s\"\n"

#: ../dos9/errors/Dos9_Errors.c:205
#, c-format
msgid "Error : Unable to delete directory \"%s\".\n"
msgstr "Erreur : Impossible de suprimmer le dossier \"%s\"\n"

#: ../dos9/errors/Dos9_Errors.c:208
#, c-format
msgid "Error : \"%s\" is not a valid codepage on your system.\n"
msgstr "Erreur : \"%s\" n'est pas une page valide sur votre système.\n"

#: ../dos9/errors/Dos9_Errors.c:211
#, c-format
msgid ""
"Error : \"%s\" is a relative path, RENAME (or REN) do not handles relative "
"path. If you want to move the file and rename it at the same time, you "
"should use MOVE command.\n"
msgstr ""
"Erreur : \"%s\" est un chemin relatif. Les commandes RENAME et REN ne "
"supportent pas les chemins relatifs. Pour déplacer et renommer un fichier de "
"façon simultanée, utilisez la commande MOVE.\n"

#: ../dos9/errors/Dos9_Errors.c:216
msgid "Error : Unable to access Dos9 main thread to perform break.\n"
msgstr ""
"Erreur : Impossible d'accéder au thread principal de Dos9 pourexécuter "
"break.\n"

#: ../dos9/errors/Dos9_Errors.c:219
#, c-format
msgid "Error : Invalid label encountered within a block (at line %d).\n"
msgstr "Erreur : étiquette rencontrée à l'intérieur d'un block (ligne %d).\n"

#: ../dos9/errors/Dos9_Errors.c:222
#, c-format
msgid "Error : Unable to lock mutex (%s).\n"
msgstr "Erreur : Impossible de saisir le mutex (%s).\n"

#: ../dos9/errors/Dos9_Errors.c:225
#, c-format
msgid "Error : Unable to release mutex (%s).\n"
msgstr "Erreur : Impossible de libérer le mutex (%s).\n"

#: ../dos9/errors/Dos9_Errors.c:228
#, c-format
msgid "Error : Unable to catenate file \"%s\".\n"
msgstr "Erreur : Impossible de concaténer \"%s\".\n"

#: ../dos9/errors/Dos9_Errors.c:231
msgid ""
