#the list of the sources
SRC=./dump.c

TARGET=../bin_${arch}/cmd/dump
OBJ=$(SRC:.c=.o)

# build all
all: $(TARGET)

# build the main lib
$(TARGET) : $(OBJ)
			@CC@ @CFLAGS@ -o $@ $^

%.o : %.c
			@CC@ -o $@ -c $< @CFLAGS@
