#the list of the sources
SRC=./dump.c

TARGET=../bin${arch}/cmd/dump
OBJ=$(SRC:.c=.o)

CC=@CC@
CFLAGS=@CFLAGS@

# build all
all: $(TARGET)

# build the main lib
$(TARGET) : $(OBJ)
	$(CC) $(CFLAGS) -o $(TARGET) $(OBJ) 

clean:
	for i in $(SRC:.c=.o); do rm -f $$i; done;

.PHONY: clean

.SUFFIXES: .o

.c.o:
	$(CC) -o $@ -c $< $(CFLAGS)
